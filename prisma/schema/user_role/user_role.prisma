model UserRole {
  id            String            @id @default(uuid())
  name          String
  actions       Json
  users         User[]
  school_id     String
  school        School            @relation(fields: [school_id], references: [id])
  order         Int
  created_at    DateTime          @default(now())
  updated_at    DateTime          @updatedAt
  role_bindings UserRoleBinding[]
}
